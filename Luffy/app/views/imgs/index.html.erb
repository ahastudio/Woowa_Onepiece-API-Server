<p id="notice"><%= notice %></p>

<h1>Imgs</h1>

<table>
  <thead>
    <tr>
      <th>Img nm user</th>
      <th>Img nm server</th>
      <th>Img size</th>
      <th>Img up path</th>
      <th>Img dn path</th>
      <th>Ad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imgs.each do |img| %>
      <tr>
        <td><%= img.img_nm_user %></td>
        <td><%= img.img_nm_server %></td>
        <td><%= img.img_size %></td>
        <td><%= img.img_up_path %></td>
        <td><%= img.img_dn_path %></td>
        <td><%= img.ad %></td>
        <td><%= link_to 'Show', img %></td>
        <td><%= link_to 'Edit', edit_img_path(img) %></td>
        <td><%= link_to 'Destroy', img, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Img', new_img_path %>
